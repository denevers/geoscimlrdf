     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://def.isotc211.org/gwml/gwmlb#">
        <owl:versionIRI rdf:resource="http://def.isotc211.org/gwml/2018/gwmlb#"/>
